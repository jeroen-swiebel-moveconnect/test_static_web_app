{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html",
            "exclude": [
                "/assets/images/*.{png,jpg,gif}",
                "/assets/css/*",
                "/assets/scss/*",
                "/assets/fonts/*",
                "/assets/js/*",
                "/assets/*",
                "/polyfills*.*",
                "/prime*.*",
                "/*.{js,gif,ico,png,eot,svg,ttf,woff,woff2}",
                "/*.js.map"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/images/*.{png,jpg,gif}",
            "/assets/css/*",
            "/assets/scss/*",
            "/assets/fonts/*",
            "/assets/js/*"
        ]
    }
}
