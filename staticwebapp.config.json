{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html",
            "exclude": [
                "/assets/images/*.{png,jpg,gif}",
                "/assets/css/*",
                "/assets/scss/*",
                "/assets/fonts/*",
                "/assets/js/*"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/images/*.{png,jpg,gif}",
            "/assets/css/*",
            "/assets/scss/*",
            "/assets/fonts/*",
            "/assets/js/*"
        ]
    }
}
